.loading-feed-backdrop {
  width: 100%;
  @include borderradius(3px);
  //animation: backdrop 2s once;
  //-webkit-animation: backdrop 2s once;
  //-moz-animation: backdrop 2s once;

  -moz-animation-name: backdrop;
  -moz-animation-iteration-count: once;
  -moz-animation-timing-function: ease;
  -moz-animation-duration: 2.5s;

  -webkit-animation-name: backdrop;
  -webkit-animation-iteration-count: once;
  -webkit-animation-timing-function: ease;
  -webkit-animation-duration: 2.5s;
}

.loading-feed {
  width: 100%;
  @include borderradius(3px);
  min-height: 20px;
  background-position-x: 50%;
  background-position-y: 50%;
  background-repeat: no-repeat;
  text-align: center;


  -moz-animation-name: loadingFeed;
  -moz-animation-iteration-count: once;
  -moz-animation-timing-function: ease;
  -moz-animation-duration: 2.5s;

  -webkit-animation-name: loadingFeed;
  -webkit-animation-iteration-count: once;
  -webkit-animation-timing-function: ease;
  -webkit-animation-duration: 2.5s;
  
  //animation:loadingFeed 2s once;
  //-webkit-animation: loadingFeed 2s once;
  //-moz-animation: loadingFeed 2s once;
}

@-moz-keyframes loadingFeed {
    0% {
        visibility: visible;
        width: 0%;
        background-color: $orange;
        color: white;
    }
    95% {
        visibility: visible;
        width: 100%;
        background-color: $orange;
        color: white;
    }
    100% {
        visibility: hidden;
        width: 100%;
        background-color: rgba(0,0,0,0);
        color: rgba(0,0,0,0);
    }
}
@-webkit-keyframes loadingFeed {
    0% {
        visibility: visible;
        width: 0%;
        background-color: $orange;
        color: white;
    }
    95% {
        visibility: visible;
        width: 100%;
        background-color: $orange;
        color: white;
    }
    100% {
        visibility: hidden;
        width: 100%;
        background-color: rgba(0,0,0,0);
        color: rgba(0,0,0,0);
    }
}
@keyframes loadingFeed {
    0% {
        visibility: visible;
        width: 0%;
        background-color: $orange;
        color: white;
    }
    95% {
        visibility: visible;
        width: 100%;
        background-color: $orange;
        color: white;
    }
    100% {
        visibility: hidden;
        width: 100%;
        background-color: rgba(0,0,0,0);
        color: rgba(0,0,0,0);
    }
}

@-moz-keyframes backdrop {
    0% {
        visibility: visible;
        background-color: #ccc;
        color: #999;
    }
    95% {
        visibility: visible;
        background-color: #ccc;
        color: #999;
    }
    100% {
        visibility: hidden;
        background-color: rgba(0,0,0,0);
        color: rgba(0,0,0,0);
    }
}
@-webkit-keyframes backdrop {
    0% {
        visibility: visible;
        background-color: #ccc;
        color: #999;
    }
    95% {
        visibility: visible;
        background-color: #ccc;
        color: #999;
    }
    100% {
        visibility: hidden;
        background-color: rgba(0,0,0,0);
        color: rgba(0,0,0,0);
    }
}
@keyframes backdrop {
    0% {
        visibility: visible;
        background-color: #ccc;
        color: #999;
    }
    95% {
