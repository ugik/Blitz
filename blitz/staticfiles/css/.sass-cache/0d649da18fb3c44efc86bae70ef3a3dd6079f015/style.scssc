.arrow-box {
  width: 0; 
  height: 0; 
  border-top: 10px solid transparent;
  border-bottom: 11px solid transparent; 
  border-right: 16px solid $border;
  //@include ffboxshadow();
  position: absolute;
  float: left;
  margin-left: -1px;
  margin-top: -10px;
}

.arrow-left {
  width: 0; 
  height: 0; 
  border-top: 10px solid transparent;
  border-bottom: 10px solid transparent; 
  border-right: 15px solid rgba(253, 253, 253, 1.0);
  //@include ffboxshadow();
  //position: absolute;
  float: left;
  margin-left: -13px;
  margin-top: 20px;
}

.green-arrow-left {
  width: 0; 
  height: 0; 
  border-top: 10px solid transparent;
  border-bottom: 10px solid transparent; 
  border-right: 15px solid $navbarblue;
  //@include ffboxshadow();
  //position: absolute;
  float: left;
  margin-left: -13px;
  margin-top: 20px;
}

.trainer-arrow {
  width: 0; 
  height: 0; 
  border-top: 10px solid transparent;
  border-bottom: 10px solid transparent; 
  border-right: 15px solid $trainercommentcolor;
  //@include ffboxshadow();
