.fullwidth {width: 100%;}


.macro-history {
  //@include ffboxshadow();
    table {
    width: 100%;
    text-align: center;
    border-collapse:collapse;
    table-layout: fixed;
    td {
      border: 1px solid #ccc;
      width: 14%;
      background: #fafbfc;
    }
  }
  .week-name {
    text-align: center;
    padding: 10px 20px;
    background: $navbarblue;
    color: white;
    @include boldfont();
  }
  .day-of-month {
    color: #999;
    font-size: 12px;
  }
}


#main-feed-controls {
  text-align: center;
}

.which-flame {
  background: #fafcfb;
  border-bottom: 1px solid rgba(0, 0, 0, 0.05);
  webkit-box-shadow: 0px 0px 2px 1px rgba(0, 0, 0, 0.1);
  -moz-box-shadow: 0px 0px 2px 1px rgba(0, 0, 0, 0.1);
  box-shadow: 0px 0px 2px 1px rgba(0, 0, 0, 0.1);
  color: #515352;
  padding: 15px;
}

.strong { @include boldfont(); }

.inbox-view {
  overflow: auto;
  background: rgba(253, 253, 253, 1.0);
  border: $border;
  .thread-list {
    float: left;
    width: 25%;
    box-sizing: border-box;
    min-height: 500px;
    max-height: 600px;
    overfloy-y: scroll;
    //border-right: $border;
  }
  .message-content {
    float: left;
    width: 75%;
    box-sizing: border-box;
    border-left: $border;
  }
}

.new-message-form {
  .to-container {
    overflow: auto;
    border-bottom: $border;
    label {
      font-size: 16px;
      line-height: inherit;
      float: left;
      @include boldfont();
      padding: 14px;
      margin: 0;
    }
    input {
      font-size: 16px;
      line-height: inherit;
      float: left;
      width: 80%;
      margin: 0;
      padding: 14px;
      @include noinputstyle();
      @include regularfont();
    }
  }
  .content-container {
    border-bottom: $border;
    textarea {
      @include noinputstyle();
      width: 100%;
      box-sizing: border-box;
      padding: 14px;
      @include regularfont();
      min-height: 365px;
    }
  }

  .send-container {
    text-align: right;
    padding: 10px;
  }
}

.thread-summary {
  overflow: auto;
  border-bottom: $border;
  display: block;
  background-color: rgba(252,252,252,1);
  padding: 10px;
  .headshot {
    margin: 5px;
    float: left;
    width: 20%;
    @include mobile-only {width: 80%;};
    box-sizing: border-box;
    img {border-radius: 50%; moz-border-radius: 50%; webkit-border-radius: 50%; o-border-radius: 50%; ms-border-radius: 50%; @include ffboxshadow();}
  }
  .content {
    float: left;
    width: 75%;
    box-sizing: border-box;
    padding: 12px 0px 0px 20px;
    color: rgba(153, 153, 153, 1.0);
    @include mobile-only {display: none;};
    .name {
      @include boldfont();
      font-size: 18px;
    }
    .date {
      font-size: 13px;
      margin-top: 5px;
      opacity: 0.7;
    }
  }
   -webkit-transition: all .75s ease;
  -moz-transition: all .75s ease;
  -ms-transition: all .75s ease;
  -o-transition: all .75s ease;
  transition: all .75s ease;
}

.thread-summary:hover {
  background-color: rgba(255, 255, 255, 1.0);
  color: #333;
  .name {color: #333;}
}

.thread-summary-active {
  background: rgba(255, 255, 255, 1.0);
  color: #333;
  .name {color: #333;
  }
  .date {color: #999;}
}

p.no-messages {
  text-align: center;
  padding: 50px 0;
  color: rgba(153, 153, 153, 1.0);
}

#messages {
  overflow: scroll;
  border-bottom: $border;
  height: 440px;
}

.message {
  padding: 20px;
  border-bottom: $border;
  .header {
    padding-bottom: 20px;
    .headshot {
      img { 
        float: left; 
        margin-right: 20px; 
        width: 40px; 
        border-radius: 50%; 
        moz-border-radius: 50%; 
        webkit-border-radius: 50%; 
        o-border-radius: 50%; 
        ms-border-radius: 50%; 
        @include ffboxshadow();}
        @include mobile-only {display: none;};
      }
    .name {
      font-size: 18px;
      @include boldfont();
      padding-top: 10px;
      @include mobile-only {};
    }
    .date {
      float: right;
      @include mobile-only {};
      color: rgba(153, 153, 153, 1.0);
      padding-top: 10px;
    }
  }
  
  .content {
    margin-left: 60px; 
    @include mobile-only {margin-left: 0px;};
    margin-top: 0px;}

}

.reply-form {
  float: left;
  width: 95%;
  display: block;
  padding-left: 20px;
  padding-top: 20px;
  //margin-bottom: 20px;
  background-color: #fffbfc;
  textarea {
    @include regularfont();
    border: $border;
    font-size: 16px;
    padding: 10px;
    width: 100%;
    box-sizing: border-box;
  }
  button {
    float: right;
  }
}

.topright-controls {
  float: right;
}

.sidebar-box-separator {
  height: 15px;
}

span.num-unread-threads {
  font-size: 14px;
  @include boldfont();
  color: white;
  background: rgba(255, 117, 0, 1.0);
  @include borderradius(20px);
  padding: 2px 6px;
}

.unread-icon {
  color: rgba(255, 117, 0, 1.0);
}

a.feed-permalink {
  float: right;
  padding: 4px;
  color: white;
}

a.feed-permalink-grey {
  color: #d3d3d3;
